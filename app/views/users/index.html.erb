<%= render 'users/userlist', user: @user %>
<%= render '/books/booklist', book: @book %>

<h2>Users</h2>
 <%= @users.each do |user| %>
 
  <table class="table table-hover table-inverse">
    <thead>
     <tr>
      <th>image</th>
      <th>name</th>
      <th></th>
      <th colspan="3"></th>
     </tr>
    </thead>
    
    <tbody>
      <p><%= image_tag @user.get_profile_image(80,80) %></p>
      <p><%= @user.name %> </p>
      <%= link_to "Show", user_path(@user.id) %>
    </thead>
   </table>
  </div>
 </div>
</div>
 
<% end %>

 